             <!-- Info Cards -->
<div class="info-cards" style="display: flex; gap: 20px; margin-top: 20px;">
    <div class="container mt-4" style="background-color: white;">
      <h2>Billing Addresses</h2>
  
      <!-- List of Addresses -->
      <div id="addressList">
        <% address.forEach((address, index) => { %>
          <div class="address-card mb-3">
            <h6>Billing Address <%= index + 1 %></h6>
            <p>
              <%= address.street %>, <%= address.city %>, <%= address.state %>, 
              <%= address.country %>, <%= address.postalCode %>, <%= address.phone %>
            </p>
            
            <!-- Radio button to select the address -->
            <input 
              type="radio" 
              name="selectedAddress" 
              class="address-checkbox" 
              value="<%= address._id %>" 
              data-street="<%= address.street %>" 
              data-city="<%= address.city %>" 
              data-state="<%= address.state %>" 
              data-country="<%= address.country %>" 
              data-postalcode="<%= address.postalCode %>" 
              data-phone="<%= address.phone || '' %>">
            Select this address
            
            <!-- Remove Button -->
            <form action="/remove-address/<%= address._id %>" method="post" style="display: inline;">
                <button type="submit" class="btn btn-link text-danger" style="font-size: 30px;  border: none;margin-left: 20px; background: none; cursor: pointer;">&times;</button>
              </form>
          </div>
        <% }) %>
      </div>
      

      <button 
      class="mb-3"
      type="button" 
      class="btn btn-primary mt-3" 
      onclick="populateModal(event)">
      Edit Selected Address
    </button>

    </div>

   
   


  </div>
  
                  
                    
                  
