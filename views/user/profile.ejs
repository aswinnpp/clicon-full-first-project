<script>
    function selectAddress(selectedCheckbox) {

      const checkboxes = document.querySelectorAll('.address-checkbox');
      
      checkboxes.forEach(checkbox => {

        if (checkbox !== selectedCheckbox) {
          checkbox.checked = false;
        }
      });
    }

    function populateModal(event) {
  
  event.preventDefault();

  const selectedRadio = document.querySelector('.address-checkbox:checked');

  if (!selectedRadio) {
   
    Swal.fire('Please select an address to edit.');
    return;
  }

  
  const addressId = selectedRadio.value;
  const street = selectedRadio.getAttribute('data-street');
  const city = selectedRadio.getAttribute('data-city');
  const state = selectedRadio.getAttribute('data-state');
  const country = selectedRadio.getAttribute('data-country');
  const postalCode = selectedRadio.getAttribute('data-postalcode');
  const phone = selectedRadio.getAttribute('data-phone');

 
  document.getElementById('editStreet').value = street;
  document.getElementById('editCity').value = city;
  document.getElementById('editState').value = state;
  document.getElementById('editCountry').value = country;
  document.getElementById('editPostalCode').value = postalCode;
  document.getElementById('editPhone').value = phone;

  
  const modal = new bootstrap.Modal(document.getElementById('editAddressModal'));
  modal.show();
}


  </script>

