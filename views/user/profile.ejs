              <!-- Modal for Adding Address -->
        <div class="modal fade" id="addAddressModal" tabindex="-1" aria-labelledby="addAddressModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
      <div class="modal-content shadow-lg rounded-3">
        
        <!-- Modal Header -->
        <div class="modal-header border-bottom-0">
          <h5 class="modal-title" id="addAddressModalLabel">Add New Address</h5>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        
        <!-- Modal Body -->
        <div class="modal-body p-4">
          <form action="/address" method="post">
            <input type="text" name="id" value="<%= id %>" hidden>
            <!-- Street Address -->
            <div class="mb-3">
              <label for="street" class="form-label fs-5 text-secondary">Street Address</label>
              <input type="text" class="form-control p-3 border-2" name="street" id="street" placeholder="Enter street address" required>
            </div>
            
            <!-- City -->
            <div class="mb-3">
              <label for="city" class="form-label fs-5 text-secondary">City</label>
              <input type="text" class="form-control p-3 border-2" name="city" id="city" placeholder="Enter city" required>
            </div>

            <div class="mb-3">
                <label for="phone" class="form-label fs-5 text-secondary">Phone no</label>
                <input type="text" class="form-control p-3 border-2" name="phone" id="city" placeholder="Enter phone no." required>
              </div>
            
            <!-- State -->
            <div class="mb-3">
              <label for="state" class="form-label fs-5 text-secondary">State</label>
              <input type="text" class="form-control p-3 border-2" name="state" id="state" placeholder="Enter state" required>
            </div>
            
            <!-- Country -->
            <div class="mb-3">
              <label for="country" class="form-label fs-5 text-secondary">Country</label>
              <input type="text" class="form-control p-3 border-2" name="country" id="country" placeholder="Enter country" required>
            </div>
            
            <!-- Postal Code -->
            <div class="mb-3">
              <label for="postalCode" class="form-label fs-5 text-secondary">Postal Code</label>
              <input type="text" class="form-control p-3 border-2" name="postalCode" id="postalCode" placeholder="Enter postal code" required>
            </div>
            
            <!-- Submit and Close Buttons -->
            <div class="d-flex justify-content-between mt-3">
              <button type="submit" class="btn btn-success px-4 py-2 rounded-3 shadow-sm">Add Address</button>
              <button type="button" class="btn btn-secondary px-4 py-2 rounded-3 shadow-sm" data-bs-dismiss="modal">Close</button>
            </div>
          </form>
        </div>
        
      </div>
    </div>
